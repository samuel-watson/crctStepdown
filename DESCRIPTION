Package: crctStepdown
Title: Univariate Analysis of Cluster Trials with Multiple Outcomes
Version: 0.5.1
Date: 2023-09-12
Authors@R: 
    person(given = "Sam",
           family = "Watson",
           role = c("aut", "cre"),
